<!-- Product Section Begin -->
<section class="product spad" id="product-list"   >
    <div class="container">
        <div class="row">
            <div class="col-lg-4 col-md-4">
                <div class="section-title">
                   
                </div>
            </div>
            <div class="col-lg-8 col-md-8">
                <ul class="filter__controls">
                    {{!-- <li class="active" data-filter="*">All</li> --}}
                    {{!-- {{#each data}}
                    <li data-filter=".women"><a style="color: black;" href="/{{this._id}}">{{this.name}}</a></li>
                    {{/each}} --}}
                    {{!-- <li data-filter=".men">Malayalam</li>
                    <li data-filter=".">Hindi</li> --}}
                    {{!-- <li data-filter=".accessories">Accessories</li>
                    <li data-filter=".cosmetic">Cosmetics</li> --}}
                </ul>
            </div>
        </div>

        <div class="row property__gallery">

            {{#each productInfo}}
            <div class="  col-lg-3 col-md-6 col-sm-6 mix women">
                <div class="product__item ">

                    
                    <div class="product__item__pic set-bg"  data-setbg="/product-images/{{this._id}}.jpg"
                        id="wishlistRefresh" style="width:90% ; ,height:auto">
                        
                        <ul class="product__hover">

                            <li><a href="/product-images/{{this._id}}.jpg" class="image-popup"><span
                                        class="arrow_expand"></span></a></li>


                            <li>
                                <a onclick="addToWishlist('{{this._id}}')"
                                    title="AddToWishlist"><i class="bi bi-heart"></i></a> 
                            </li>


                            <li>

                                <a onclick="addToCart('{{this._id}}')"
                                    title="AddToCart"><i class="bi bi-bag-check"></i></a>
                            </li>
                        </ul>
                    </div>
                    <div class="product__item__text">
                        <h6><a href="/view-details/{{this._id}}">{{this.name}}</a></h6>
                        <div class="rating">
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                        </div>
                        <div class="product__price">₹{{this.price}}</div>
                    </div>
                </div>
            </div>

            {{/each}}

        </div>
    </div>
</section>
<!-- Product Section End -->



